        window.myLine = new Chart(ctx, config);

        $("#lideres > li > a").on('click', function (evt) {
            var entity = $(this).attr('id');
            
            $.get("/summary/comments/" + entity, function (data) {
                console.log(data.monthLabels);
                // Negative
                var negative = []
                for (var i = 0; i < data.monthSummaries.length; i++) {
                    negative.push(data.monthSummaries[i].negative);
                }
                config.data.datasets[0].data = negative;
                // Positive
                var positive = []
                for (var i = 0; i < data.monthSummaries.length; i++) {
                    positive.push(data.monthSummaries[i].positive);
                }
                config.data.datasets[1].data = positive;
                // Neutral
                var neutral = []
                for (var i = 0; i < data.monthSummaries.length; i++) {
                    neutral.push(data.monthSummaries[i].neutral);
                }
                config.data.datasets[2].data = neutral;

                /*config.data.datasets.forEach(function(dataset) {
